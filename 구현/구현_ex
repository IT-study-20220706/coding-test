//이차원리스트 90도 변환하 결과 반환 메소드
  public static int[][] rotate_90_Degree(int[][] a) {
      int n = a.length;
      int m = a[0].length;
      int[][] result = new int[n][m]; // 결과 리스트
      for (int i = 0; i < n; i++) {
          for (int j = 0; j < m; j++) {
              result[j][n - i - 1] = a[i][j];
          }
      }
      return result;
  }


